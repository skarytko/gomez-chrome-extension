{
	"name": "Dynatrace APMaaS Portal Login Manager",
	"version": "3.0.0",
	"description": "Manage usernames/passwords for your favorite Dynatrace APMaaS accounts",
	"manifest_version": 2,
	"options_page": "options.html",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Dynatrace APMaaS Portal Login Manager",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "icon-small.png",
		"48": "icon-medium.png",
		"128": "icon-large.png"
	},
	"permissions": [
		"storage",
		"tabs",
		"cookies",
		"http://*/*",
		"https://*/*"
	]
}